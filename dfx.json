{
  "canisters": {
    "bitcoin_customs": {
      "gzip": true,
      "candid": "customs/bitcoin/bitcoin_customs.did",
      "package": "bitcoin_customs",
      "type": "rust"
    },
    "omnity_hub": {
      "gzip": true,
      "candid": "hub/omnity_hub.did",
      "package": "omnity_hub",
      "type": "rust"
    },
    "hub_mock": {
      "gzip": true,
      "candid": "mock/hub/hub_mock.did",
      "package": "hub_mock",
      "type": "rust"
    }
  },
  "defaults": {
    "bitcoin": {
      "enabled": true,
      "nodes": [
        "127.0.0.1:18444"
